<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unitec.flashlight_application">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconlight"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".LightActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".NeOnActivity"
            android:label="@string/app_name" />
        <activity android:name=".WaringActivity" />
        <activity android:name=".LightTraffic"></activity>
    </application>

</manifest>